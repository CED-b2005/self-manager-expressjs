generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        String   @id @default(uuid())  // Chỉnh sửa id thành UUID
  email     String   @unique
  name      String   @unique
  role      String
  password  String
  isActive  Boolean  @default(true)  // Đặt mặc định cho isActive là true
}